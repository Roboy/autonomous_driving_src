<robot name="rickshaw">
  <material name="grey">
    <color rgba="0.8 0.8 0.8 1"/>
  </material>
  <material name="white">
    <color rgba="1 1 1 1 "/>
  </material>
  <material name="black">
    <color rgba="0 0 0 1 "/>
  </material>

  <!-- Base link -->
  <link name="base_link">
    <inertial>
      <mass value="21.81948036288467"/>
      <origin rpy="0 0 0" xyz="-0.83471  0.03437  0.037"/>
      <inertia ixx="10.27568843951201" ixy="-0.013973690995530063"
               ixz="-0.24730354425384926" iyy="13.85328568075199"
               iyz="-3.7960118868960127" izz="4.2009117489265355"/>
    </inertial>
    <collision name="bike_collision">
      <origin rpy="0 0 0" xyz="-0.83471  0.03437  0.037"/>
      <geometry>
        <mesh
            filename="package://roboy_models/rickshaw/meshes/collisions/bike.stl"
            scale="0.001 0.001 0.001"/>
      </geometry>
    </collision>
    <visual name="bike_visual">
      <origin rpy="0 0 0" xyz="-0.83471  0.03437  0.037"/>
      <geometry>
        <mesh filename="package://roboy_models/rickshaw/meshes/visuals/bike.stl"
              scale="0.001 0.001 0.001"/>
      </geometry>
      <material name="black"/>
    </visual>
  </link>

  <link name="left_wheel">
    <inertial>
      <mass value="13.340163929097852"/>
      <origin rpy="0 0 0" xyz="0    -0.046  0"/>
      <inertia ixx="2.6886582542943342" ixy="-3.73354366648907"
               ixz="1.0631600153408745" iyy="12.355706573664577"
               iyz="2.986802675143825" izz="12.327019793799982"/>
